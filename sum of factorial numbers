# sum of factorial numbers
def factorial(x):
    result = 1
    for i in range(1, x+1):
        result *= i
    return result

m = int(input("enter the value of m:"))
n = int(input("enter the value of n:"))
if m > n:
    print("0")
else:
    sum_fact = 0
    for i in range(m, n+1):
        sum_fact += factorial(i)
    print("sum of factorial:", sum_fact)
